{
  "name": "nog-sync-app",
  "private": true,
  "scripts": {
    "start": "meteor run",
    "lint": "eslint ."
  },
  "dependencies": {
    "meteor-node-stubs": "~0.2.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "meteor"
    ],
    "extends": [
      "airbnb",
      "plugin:meteor/recommended"
    ],
    "rules": {
      "import/no-unresolved": [
        "error",
        {
          "ignore": [
            "^meteor/"
          ]
        }
      ]
    }
  },
  "author": "Steffen Prohaska <prohaska@zib.de>",
  "license": "MIT",
  "//": "eslint-plugin-import is locked to 1.8.x, because 1.10.1 did not work",
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "eslint": "^2.13.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "~1.8.1",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-meteor": "^3.6.0",
    "eslint-plugin-react": "^5.2.2"
  }
}
